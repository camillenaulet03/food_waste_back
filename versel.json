{
  "version": 2,
  "buildCommand": "nodemon -r dotenv/config index.js",
  "functions": {
    "api/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "controllers/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "log/*.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "models/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://food-waste-back.vercel.app"
    }
  ]
}
