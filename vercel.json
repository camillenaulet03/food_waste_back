{
  "version": 2,
  "redirects": [
    {
      "source": "/api/wastes",
      "destination": "/api/routes/waste",
      "permanent": false
    },
    {
      "source": "/api/auth",
      "destination": "/api/routes/user",
      "permanent": false
    },
    {
      "source": "/soap",
      "destination": "/api/routes/soapSubstract",
      "permanent": false
    }
  ],
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "index.js", "use": "@vercel/node" }
  ]
}
