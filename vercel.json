{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "redirects": [
    {
      "source": "/api/wastes",
      "destination": "/routes/waste.js"
    },
    {
      "source": "/api/auth",
      "destination": "/routes/user.js"
    },
    {
      "source": "/soap",
      "destination": "/routes/soapSubstract.js"
    }
  ]
}
