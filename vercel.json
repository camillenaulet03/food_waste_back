{
  "version": 2,
  "redirects": [
    {
      "source": "/api/wastes",
      "destination": "/routes/waste",
      "permanent": false
    },
    {
      "source": "/api/auth",
      "destination": "/routes/user",
      "permanent": false
    },
    {
      "source": "/soap",
      "destination": "/routes/soapSubstract",
      "permanent": false
    }
  ],
  "functions": {
    "**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
