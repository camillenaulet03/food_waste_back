{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "redirects": [
    {
      "source": "/api/wastes",
      "destination": "/routes/waste",
      "permanent": false
    },
    {
      "source": "/api/auth",
      "destination": "/routes/user",
      "permanent": false
    },
    {
      "source": "/soap",
      "destination": "/routes/soapSubstract",
      "permanent": false
    }
  ],
  "functions": {
    "**/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
